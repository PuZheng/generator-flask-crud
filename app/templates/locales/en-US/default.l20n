<warning_title "Warning!">
<remove_question {
    *object: "Are you sure to remove this {{ $modelName }}?",
    list: "Are you sure to remove the selected record(s)?"
}>
<success_title "Success!">
<delete_success "{{ $modelName }} \"%s\" has been removed!">
<error_title "Error!">
<remove_failed {
    *object: "Can't remove {{ $modelName }} \"%s\"!",
    list: "Failed to remove selected record(s)!"
}>
<info_title "Hint">
<remove_hint "Please select at least one record to remove!">
<cancel_remove "Do not remove!">
<remove_success "Selected records have been removed!">
